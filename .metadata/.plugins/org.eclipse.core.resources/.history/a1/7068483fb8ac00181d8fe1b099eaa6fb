package footballScoresExample;

import java.util.ArrayList;

public class Practice {

	public static void main(String[] args) {
		ArrayList myArray = new ArrayList();
		myArray.add("John");
		myArray.add("Bryant");
		myArray.add("Lewis");
		myArray.add(1);
		
		System.out.println(myArray);
		
		
	}
	
}


